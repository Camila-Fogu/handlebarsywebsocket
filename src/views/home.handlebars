<h2>PRODUCTOS</h2>
{{#if products.length}}
  {{#each products}}
    <div>
      <p>{{category}}</p>
      <p>{{title}}</p>
      <p>{{description}}</p>
      <p>Precio: {{price}}</p>
      <p>Stock disponible: {{stock}}</p>
    </div>
  {{/each}}
{{else}}
  <h3>No hay productos disponibles</h3>
{{/if}}