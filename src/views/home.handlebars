<h2>PRODUCTOS</h2>
{{#if products.length}}
  {{#each}}
    <div>
      <p>{{product.category}}</p>
      <p>{{product.title}}</p>
      <p>{{product.description}}</p>
      <p>Precio: {{product.price}}</p>
      <p>Stock disponible: {{product.stock}}</p>
    </div>
  {{/each}}
{{else}}
  <h3>No hay productos disponibles</h3>
{{/if}}